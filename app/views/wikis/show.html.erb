<div class="wrapper">
  <div class="header">
    <%= render "layouts/header" %>
  </div>
  <div class="menu">
    <%= render "layouts/menu" %>
  </div>
  <div class="content">
    <div class="row">
      <h1><%= @wiki.title %></h1>
      <div class="col-md-8">
        <p><%= @wiki.body %></p>
      </div>

      <div class="col-md-4">
        <%= link_to "Edit wiki", edit_wiki_path(@wiki), class: 'btn btn-success' %>
      </div>

      <div class="col-md-4">
        <%= link_to "Delete Post", [@wiki], method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this post?' } %>
      </div>

    </div>
  </div>
  <%= render "layouts/footer" %>
</div>





