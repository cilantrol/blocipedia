<div class= 'form-group'>
    <%= form_for([@collaborator, @collaborator.user.build]) do |f| %>
      <%  @users.each do |user| %>
        <li><%= f.check_box 'emails[]', user.email %>
            <%= user.email %>
        </li>
      <% end %>
      <%= f.submit 'click to add' %>
    <% end %>
</div>